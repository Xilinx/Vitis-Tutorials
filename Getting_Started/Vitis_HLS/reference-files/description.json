 {
    "name": "Vitis_HLS",
    "description": "Getting Started with Vitis HLS",
    "flow": "vitis",
 
    "platform_allowlist": ["u200"],
    "platform_blocklist": ["u280"],
 
    "testinfo": {
        "test_name": "GS_hls",
        "jobs": [
            {
                "max_memory_MB": 32768,
                "max_time_min": {
                    "vitis_sw_emu": 30,
                    "vitis_hw_emu": 200,
                    "vitis_hw_build": 470,
                    "vitis_hw_run": 30
                }
            }
        ],
        "targets": [
             "vitis_sw_emu"
        ],
        "make_options" : [ "TARGET='$user_test_mode'", "XF_PROJ_ROOT='${GITHUB_LIBS_REPO}/Vitis-Tutorials'", "PLATFORM_REPO_PATHS='$PLATFORM_PATH'" ],
        "category": "canary",
        "custom_build_target": {
            "all": "run",
            "hw": "xclbin"
        }
    }
}
