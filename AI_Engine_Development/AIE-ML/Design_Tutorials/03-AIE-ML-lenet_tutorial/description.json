{
    "name": "AI_Engine_Development-03-AIE-ML-lenet_tutorial",
    "description": "lenet_aie_ml_tutorial",
    "flow": "versal",
    
    "platform_allowlist": [ "vek280"],
    "platform_blocklist": [],
    
    "testinfo": {
        "test_name": "AIE_ML_D_03-LeNet",
        "tasks": {
           "build": {
               "pre_exec": "./env_setup_versal.sh"
           },
           "board": {
               "pre_exec": "./env_setup_versal.sh",
               "post_exec": "./addon/copy_perf_files.sh"
           }
        },
        "user": {
           "sync_sd_card":1
        },
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 76800,
                "max_time_min": {
                    "vitis_aie_sim": 50,
                    "vitis_aie_x86sim": 50,
                    "vitis_hw_build": 500,
                    "vitis_hw_emu": 960,
                    "vitis_hw_run": 60
                }
            }
        ],
        "targets": [ 
            "vitis_hw_build",
            "vitis_hw_run",
            "vitis_hw_emu"
        ],
        "category": "canary",
        "make_options" : [ 
           "EN_TRACE=1",
           "LAUNCH_HW_EMU_EXEC=1"
           ],
        "custom_build_target": {
            "all": "run",
            "hw": "sd_card"
        },
        "custom_board_target": "run_test",
        "customized_params": "multi_params.json"
    }
}
